
@{
    ViewData["Title"] = "Index";
  
}
@model IEnumerable<InventoryManagementSystem.Controllers.CategoryEndponits.CategoryController.cat>
<h1>Index</h1>



<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var cat in Model)
        {
            <tr>
                <td>@cat.ID</td>
                <td>@cat.Name</td>
            </tr>
        }
    </tbody>
</table>

